{
    "$schema": "http://json-schema.org/schema",
    "$id": "SchematicsLogin",
    "title": "Login Schema",
    "type": "object",
    "properties": {
        "project": {
            "type": "string",
            "description": "Name of the project.",
            "$default": {
                "$source": "projectName"
            }
        },
        "component": {
            "description": "Name of the login component class name.",
            "type": "string",
            "default": "LoginComponent"
        }
    },
    "required": ["project", "component"]
}
