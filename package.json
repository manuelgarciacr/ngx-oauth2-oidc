{
    "name": "ngx-oauth2-oidc",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --project sample --port 4300 -c development -o",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "CHROME_BIN=/usr/bin/google-chrome-stable ng test",
        "lint": "ng lint",
        "build-oauth2": "ng build ngx-oauth2-oidc --configuration development",
        "test-oauth2": "CHROME_BIN=/usr/bin/google-chrome-stable ng test ngx-oauth2-oidc",
        "lint-oauth2": "ng lint ngx-oauth2-oidc",
        "pack-oauth2": "cd ./dist/ngx-oauth2-oidc && npm pack --pack-destination ../../",
        "publish-oauth2": "ng build ngx-oauth2-oidc && cd ./dist/ngx-oauth2-oidc && npm login && npm publish"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^18.0.0",
        "@angular/common": "^18.0.0",
        "@angular/compiler": "^18.0.0",
        "@angular/core": "^18.0.0",
        "@angular/forms": "^18.0.0",
        "@angular/platform-browser": "^18.0.0",
        "@angular/platform-browser-dynamic": "^18.0.0",
        "@angular/router": "^18.0.0",
        "jose": "^5.3.0",
        "ngx-oauth2-oidc": "file:ngx-oauth2-oidc-0.0.1.tgz",
        "pkce-challenge": "^4.1.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^18.0.1",
        "@angular/cli": "^18.0.1",
        "@angular/compiler-cli": "^18.0.0",
        "@types/jasmine": "~5.1.0",
        "angular-eslint": "18.0.0",
        "eslint": "^9.3.0",
        "jasmine-core": "~5.1.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "ng-packagr": "^18.0.0",
        "typescript": "~5.4.2",
        "typescript-eslint": "8.0.0-alpha.20"
    }
}
