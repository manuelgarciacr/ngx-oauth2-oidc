{
    "name": "ngx-oauth2-oidc",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "npm --loglevel verbose run start:demo",
        "start:demo": "ng serve --project demo --port 4400 -c development -o",
        "start:sample": "ng serve --project sample --port 4300 -c development -o",
        "start:sandbox": "ng serve --project sandbox --port 4500 -c development -o",
        "watch": "ng build --watch -c development",
        "test": "CHROME_BIN=/usr/bin/google-chrome-stable ng test --browsers=ChromeHeadless",
        "lint": "ng lint",
        "set-verbose": "npm config set loglevel=verbose",
        "set-notice": "npm config set loglevel=notice",
        "=================================": "ngx-oauth2-oidc (AKA oauth2) development",
        "build-oauth2": "ng build ngx-oauth2-oidc -c development",
        "test-oauth2": "CHROME_BIN=/usr/bin/google-chrome-stable ng test ngx-oauth2-oidc",
        "lint-oauth2": "ng lint ngx-oauth2-oidc",
        "pack-oauth2": "cd ./dist/ngx-oauth2-oidc && npm pack --pack-destination ../../",
        "==================================": "link dist/ngx-oauth2-oidc to sandbox",
        "link": "npm run link:dist && npm run link:sandbox && npm run link:modapp",
        "link:dist": "cd dist/ngx-oauth2-oidc && sudo npm link",
        "link:sandbox": "cd projects/sandbox && sudo npm link ngx-oauth2-oidc",
        "link:modapp": "cd projects/modapp && sudo npm link ngx-oauth2-oidc",
        "?????link:rm": "sudo npm rm -g ngx-oauth2-oidc",
        "===================================": "build ngx-oauth2-oidc",
        "build": "ng build ngx-oauth2-oidc",
        "====================================": "build ngx-oauth2-oidc schematics",
        "build:schematics": "tsc --version && tsc -p projects/ngx-oauth2-oidc/tsconfig.schematics.json",
        "build:schematics:dev": "tsc --version && tsc -p projects/ngx-oauth2-oidc/tsconfig.schematics.dev.json",
        "copyFiles": "copyfiles -u 2 projects/ngx-oauth2-oidc/schematics/collection.json projects/ngx-oauth2-oidc/schematics/**/files/* projects/ngx-oauth2-oidc/schematics/*/*.json dist/ngx-oauth2-oidc/",
        "postbuild:schematics": "npm --loglevel notice run copyFiles",
        "postbuild:schematics:dev": "npm --loglevel notice run copyFiles",
        "=====================================": "pack and publish ngx-oauth2-oidc",
        "publish": "npm run pack && npm login && cd dist/ngx-oauth2-oidc && npm publish --access=public",
        "pack": "npm run build && npm run build:schematics && cd dist/ngx-oauth2-oidc && npm pack",
        "======================================": "test ngx-oauth2-oidc schematics",
        "exec": "npm --loglevel notice run build:schematics:dev && npm --loglevel verbose run test:sta && npm --loglevel verbose run test:mod",
        "exec2": "npm --loglevel notice run build:schematics:dev && cd projects/sandbox && ng generate ngx-oauth2-oidc:login --component loginComponent",
        "exec:mod": "npm --loglevel notice run build:schematics:dev && npm --loglevel verbose run test:mod",
        "exec:sta": "npm --loglevel notice run build:schematics:dev && npm --loglevel verbose run test:sta",
        "---test:mod": "cd projects/modapp && ng generate ngx-oauth2-oidc:login --component loginComponent",
        "test:mod": "cd projects/modapp && ng generate ngx-oauth2-oidc:guard --route foo",
        "---test:sta": "cd projects/sandbox && ng generate ngx-oauth2-oidc:login --component loginComponent",
        "test:sta": "cd projects/sandbox && ng generate ngx-oauth2-oidc:guard --route foo",
        "test:schematics": "npm --loglevel notice run build:schematics:dev && jasmine --verbose dist/ngx-oauth2-oidc/schematics/**/*.spec.js"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^18.0.0",
        "@angular/cdk": "^18.2.4",
        "@angular/common": "^18.0.0",
        "@angular/compiler": "^18.0.0",
        "@angular/core": "^18.0.0",
        "@angular/forms": "^18.0.0",
        "@angular/material": "^18.2.4",
        "@angular/platform-browser": "^18.0.0",
        "@angular/platform-browser-dynamic": "^18.0.0",
        "@angular/router": "^18.0.0",
        "@ng-bootstrap/ng-bootstrap": "^17.0.1",
        "@popperjs/core": "^2.11.8",
        "bootstrap": "^5.3.3",
        "jose": "^5.3.0",
        "pkce-challenge": "^4.1.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^18.0.1",
        "@angular/cli": "^18.2.12",
        "@angular/compiler-cli": "^18.0.0",
        "@angular/localize": "^18.2.9",
        "@types/angular-material": "^1.1.77",
        "@types/jasmine": "~5.1.0",
        "angular-eslint": "18.0.0",
        "copyfiles": "^2.4.1",
        "detect-indent": "^7.0.1",
        "eslint": "^9.3.0",
        "jasmine": "^5.5.0",
        "jasmine-core": "^5.5.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "^3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "^5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "ng-packagr": "^18.0.0",
        "typescript": "~5.4.2",
        "typescript-eslint": "8.0.0-alpha.20"
    }
}
